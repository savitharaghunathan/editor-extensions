{
  "name": "@editor-extensions/agentic",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "ci": "npx jest",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean --sourcemap",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch --sourcemap"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts}": "eslint --fix",
    "*.json": "prettier --write"
  },
  "dependencies": {
    "@langchain/core": "^1.1.7",
    "@langchain/langgraph": "^1.0.0",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tsup": "^8.4.0"
  }
}
